{
    "name": "itsmemanoj/shiprocket-laravel",
    "description": "Shiprocket API (V1) Laravel SDK. Integration of Shiprocket API in your laravel application is easy, This package enables you to utilize most of your Shiprocket functions.",
    "keywords": [
        "itsmemanoj",
        "laravel Shiprcoket Api",
        "Laravel Shiprocket Wrapper",
        "Shiprocket SDK Module"
    ],
    "homepage": "https://github.com/itsmemanoj/shiprocket-laravel",
    "license": "MIT",
    "authors": [{
        "name": "Itsmemanoj",
        "email": "itsmemanoj.bisht@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "ItsmeManoj\\Shiprocket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ItsmeManoj\\Shiprocket\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ItsmeManoj\\Shiprocket\\ShiprocketServiceProvider"
            ],
            "aliases": {
                "Shiprocket": "ItsmeManoj\\Shiprocket\\Shiprocket"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
